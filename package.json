{
  "name": "@shor/compliance",
  "version": "1.0.0",
  "description": "Shor Compliance Framework - SDK and CLI for blockchain compliance",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "test": "npm run test --workspaces",
    "lint": "npm run lint --workspaces",
    "dev": "npm run dev --workspace=@shor/cli",
    "clean": "npm run clean --workspaces && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "typescript": "^5.5.3",
    "tslib": "^2.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shor-protocol/compliance-framework"
  },
  "keywords": [
    "compliance",
    "blockchain",
    "ethereum",
    "solana",
    "kyc",
    "aml",
    "regulation",
    "sec",
    "mica"
  ],
  "author": "Shor Protocol",
  "license": "Apache-2.0"
}